    "@types/vinyl": "^2.0.0",
    "@types/vinyl-buffer": "^0.0.29",
    "@types/vinyl-fs": "^2.4.5",
    "@types/vinyl-paths": "^0.0.30",
    "@types/vinyl-source-stream": "^0.0.29",
