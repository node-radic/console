{
    "name": "@radic/console",
    "version": "0.0.5",
    "description": "Console ",
    "main": "src/index.js",
    "typings": "lib/index.d.ts",
    "jsnext:main": "lib/index.js",
    "author": "Robin Radic",
    "license": "MIT",
    "repository": "https://github.com/node-radic/console",
    "scripts": {
        "build": "gulp build",
        "test": "gulp test",
        "clean": "gulp clean",
        "dev:init": "npm run dev:install; gulp build; npm run dev:docs",
        "dev:install": "rm -rf node_modules; rm yarn.lock; yarn install",
        "dev:docs": "rm -r docs &> /dev/null ; typedoc --includeDeclarations --target ES6 --exclude spec example example2 node_modules types/lodash --ignoreCompilerErrors --mode file --excludeExternals --excludePrivate --readme docs.md --out docs ./lib/index.d.ts",
        "deploy:docs": "npm run dev:docs; gulp ghpages",
        "deploy:patch": "npm run test && mversion patch -m && npm run deploy:docs",
        "deploy:minor": "npm run test && mversion minor -m && npm run deploy:docs",
        "deploy:major": "npm run test && mversion major -m && npm run deploy:docs"
    },
    "dependencies": {
        "cli-table2": "^0.2.0",
        "columnify": "latest",
        "eventemitter2": "^4.0.0",
        "fs-extra": "^3.0.1",
        "globule": "^1.1.0",
        "inquirer": "^3.0.6",
        "inversify": "^3.1.0",
        "inversify-binding-decorators": "^3.0.0",
        "inversify-inject-decorators": "^3.0.1",
        "lodash": "^4.17.4",
        "moment": "^2.18.1",
        "reflect-metadata": "^0.1.10",
        "string": "^3.3.3",
        "truwrap": "^0.7.4",
        "callsites": "^2.0.0",
        "winston": "^2.3.1",
        "archy": "^1.0.0",
        "yargs-parser": "^5.0.0"
    },
    "devDependencies": {
        "@radic/console-colors": "^0.9.0",
        "@radic/util": "^1.5.0",
        "@types/fs-extra": "^3.0.0",
        "@types/reflect-metadata": "^0.0.5",
        "@types/string": "^0.0.28",
        "@types/archy": "^0.0.30",
        "@types/commander": "^2.9.0",
        "@types/globule": "^1.1.1",
        "@types/gulp": "^3.8.32",
        "@types/gulp-util": "^3.0.31",
        "@types/inquirer": "^0.0.32",
        "@types/jasmine": "^2.2.33",
        "@types/moment": "^2.13.0",
        "@types/node": "^7.0.8",
        "@types/through2": "^2.0.32",
        "@types/vinyl": "^2.0.0",
        "@types/vinyl-fs": "^2.4.5",
        "@types/winston": "^2.3.0",
        "gulp": "^3.9.1",
        "gulp-clean": "^0.3.2",
        "gulp-gh-pages": "^0.5.4",
        "gulp-header": "1.8.8",
        "gulp-istanbul": "^1.0.0",
        "gulp-jasmine": "^2.4.1",
        "gulp-mocha": "^3.0.0",
        "gulp-rename": "^1.2.2",
        "gulp-rollup": "^2.5.1",
        "gulp-sourcemaps": "^1.6.0",
        "gulp-tslint": "^6.0.1",
        "gulp-typescript": "^3.0.0",
        "gulp-uglify": "^2.0.0",
        "gulp-util": "^3.0.8",
        "jasmine": "^2.4.1",
        "jasmine-spec-reporter": "^2.7.0",
        "pump": "^1.0.2",
        "rollup": "^0.35.12",
        "rollup-plugin-typescript": "^0.8.1",
        "rollup-plugin-uglify": "^1.0.1",
        "run-sequence": "^1.2.2",
